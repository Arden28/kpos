{
    "name": "koverae/pos",
    "type": "project",
    "description": "Koverae POS is a POS management software developped by Koverae INC.",
    "keywords": ["koverae", "pos"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "barryvdh/laravel-dompdf": "^2.0",
        "barryvdh/laravel-snappy": "^1.0",
        "bpuig/laravel-subby": "^7.0",
        "bumbummen99/shoppingcart": "^4.2",
        "fideloper/proxy": "^4.4",
        "fruitcake/php-cors": "^1.2",
        "guzzlehttp/guzzle": "^7.2",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "illuminate/console": "*",
        "laravel/framework": "^9.19",
        "laravel/pint": "^1.6",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.7",
        "livewire/livewire": "^2.12",
        "mhmiton/laravel-modules-livewire": "^1.6",
        "milon/barcode": "^10.0",
        "nwidart/laravel-modules": "^8.2",
        "ramsey/uuid": "^4.7",
        "realrashid/sweet-alert": "^6.0",
        "spatie/laravel-medialibrary": "^10.7",
        "spatie/laravel-permission": "^5.5",
        "yajra/laravel-datatables": "9.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.7",
        "laravel/dusk": "^7.1",
        "laravel/pint": "^1.6",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "Modules/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
